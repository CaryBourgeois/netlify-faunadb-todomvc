{
  "name": "netlify-faunadb-todomvc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "faunadb": "^2.5.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "build:functions": "npm run zip",
    "zip": "mkdir functions-build && cd functions/identity-signup && npm install && zip -r identity-signup.zip *",
    "postzip": "mv functions/identity-signup/identity-signup.zip functions-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "netlify-lambda": "^0.4.0",
    "npm-run-all": "^4.1.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
